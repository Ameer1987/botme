{% extends 'layout/layout.html.twig' %}

{% block body %}
    <h1>Carttype</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cartType.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ cartType.code }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ cartType.name }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{% if cartType.createdat %}{{ cartType.createdat|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{% if cartType.updatedat %}{{ cartType.updatedat|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_carttype_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_carttype_edit', { 'id': cartType.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
